 "css": "\n.print-format{\n    margin:0px;\n    padding:10px;\n}\ntable{\n    width:100%;\n    border:1px solid;\n}\ntable{\n  font-size: 15px;\n  border:1px solid;\n}\ntd{\n    height:20;\n}\ntr{\n    border-bottom:none;\n}\n",
